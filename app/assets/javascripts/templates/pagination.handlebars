{{#if total}}
  <h5 class="pager-info">{{start}} - {{end}} de {{total}} elementos</h5>
{{else}}
  <h5 class="pager-info">No se encontraron elementos</h5>
{{/if}}
<ul class="pager">
  <li {{bind-attr class="hasPrev::disabled"}}><a href="#" {{action 'previousPage'}}><i class="fa fa-angle-left"></i></a></li>
  <li {{bind-attr class="hasNext::disabled"}}><a href="#" {{action 'nextPage'}}><i class="fa fa-angle-right"></i></a></li>
</ul>
