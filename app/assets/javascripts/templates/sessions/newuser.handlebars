<div id="login-page">
      <div class="container">
    
    <div class="login-wrap" align= "center">
      <div class="col-lg-12">
        <div class="form-panel">
          <div class="form-horizontal">
              <form class="style-form" role="form" {{action "save" on="submit"}} onkeydown="Bursa.keydownFormHandler('controller:usuarios.new', 'save');">
                <div class="form-group">
                  <div class="col-lg-2"  {{bind-attr class="errors.nombre:has-error errors.nombre:has-feedback"}}>
                    <label class="form-label"  for="nombre">RUC</label>
                    {{focus-input  type="text" value=nombre class="form-control" placeholder="RUC" autofocus="autofocus"}}
                    <span class="help-block">{{error errors.nombre}}</span>
                    {{#if errors.nombre}}
                    <span class="glyphicon glyphicon-remove form-control-feedback"></span>
                    {{/if}}
                  </div>
                  <div class="col-lg-3"  {{bind-attr class="errors.nombre:has-error errors.nombre:has-feedback"}}>
                    <label class="form-label"  for="nombre">Nombre de la Empresa/Proveedor</label>
                    {{focus-input  type="text" value=nombre class="form-control" placeholder="Nombre" autofocus="autofocus"}}
                    <span class="help-block">{{error errors.nombre}}</span>
                    {{#if errors.nombre}}
                    <span class="glyphicon glyphicon-remove form-control-feedback"></span>
                    {{/if}}
                  </div>
                  <div class="col-lg-4" {{bind-attr class="errors.direccion:has-error errors.direccion:has-feedback"}}>
                    <label class="form-label"  for="direccion">Dirección</label>
                    {{input type="text" value=direccion class="form-control" placeholder="Dirección"}}
                    <span class="help-block">{{error errors.direccion}}</span>
                    {{#if errors.direccion}}
                    <span class="glyphicon glyphicon-remove form-control-feedback"></span>
                    {{/if}}
                  </div>
                  <div class="col-lg-2" {{bind-attr class="errors.telefono:has-error errors.telefono:has-feedback"}}>
                    <label class="form-label"  for="telefono">Teléfono</label>
                    {{input type="text" value=telefono class="form-control" placeholder="Teléfono"}}
                    <span class="help-block">{{error errors.telefono}}</span>
                    {{#if errors.telefono}}
                    <span class="glyphicon glyphicon-remove form-control-feedback"></span>
                    {{/if}}
                  </div>
                </div>
                <div class="form-group">
                   <div class="col-lg-4"  {{bind-attr class="errors.password:has-error errors.password:has-feedback"}}>
                    <label class="form-label" for="password">Contraseña</label>
                     {{input type="password" value=password class="form-control" placeholder="Contraseña"}}
                     <span class="help-block">{{error errors.password}}</span>
                    {{#if errors.password}}
                    <span class="glyphicon glyphicon-remove form-control-feedback"></span>
                    {{/if}}
                  </div>

