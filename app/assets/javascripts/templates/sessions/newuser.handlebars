                    {{input type="text" value=username class="form-control" placeholder="Nombre de Usuario. Ej: jPerez" }}
                     <span class="help-block">{{error errors.username}}</span>
                    {{#if errors.username}}
                    <span class="glyphicon glyphicon-remove form-control-feedback"></span>
                    {{/if}}
                  </div>
                  
